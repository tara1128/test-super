// See https://go.microsoft.com/fwlink/?LinkId=759670
// for the documentation about the jsconfig.json format
{
    "compilerOptions": {
        "target": "ES6",
        "module": "es6",
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "jsx": "react"
    },
    "exclude": [
        "dist",
        "dist-web",
        "dist-spa",
        "node_modules",
        "bower_components",
        "jspm_packages",
        "logs",
        "tmp",
        "temp"
    ]
}